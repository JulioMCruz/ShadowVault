# Environment Variables Template for ShadowVault Contracts
# Copy this content to a .env file in the ShadowVaultContracts directory

# Zircuit Testnet Configuration
ZIRCUIT_RPC_URL=https://rpc.zircuit.com

# Wallet Configuration
# IMPORTANT: Replace with your actual private key (without 0x prefix)
# Example: PRIVATE_KEY=1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Optional: Gas Reporter
REPORT_GAS=true

# Optional: Etherscan API Key (if you want to verify contracts)
# ZIRCUITSCAN_API_KEY=your_zircuitscan_api_key_here

# Optional: Base Sepolia (for reference)
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
BASESCAN_API_KEY=your_basescan_api_key_here
